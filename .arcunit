{
  "engines": {
    "proto-gen-checker": {
      "type": "proto-gen-checker",
      "include": [
         "(\\.proto$)"
      ],
      "exclude": [
        "(^third_party/)"
      ]
    },
    "graphql-gen-checker": {
      "type": "graphql-gen-checker",
      "include": [
         "(\\.graphql$)"
      ],
      "exclude": [
        "(^third_party/)"
      ]
    },
    "go-gen-checker": {
      "type": "go-gen-checker",
      "include": [
         "(\\.go$)"
      ],
      "exclude": [
        "(^third_party/)"
      ]
    }
  }
}
